[MASTER]
init-hook='import sys; sys.path.append("./")'
max-line-length=150
disable=broad-except,broad-exception-raised
min-public-methods=0
fail-under=9.5
extension-pkg-allow-list=pydantic
max-locals=25
max-args=10
max-branches=20
max-nested-blocks=6